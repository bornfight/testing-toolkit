{
  "name": "degordian/testing-toolkit",
  "description": "A collection of various testing and static-analysis tools that are used internally.",
  "type": "metapackage",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/krukru/yii2-coding-standards"
    }
  ],
  "require": {
    "codeception/codeception": "*",
    "codeception/specify": "*",
    "codeception/verify": "*",
    "codeception/c3": "*",
    "codeception/mockery-module": "*",
    "php-mock/php-mock-mockery": "*",
    "league/factory-muffin": "*",
    "league/factory-muffin-faker": "*",
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "*",
    "phpmetrics/phpmetrics": "*",
    "sebastian/phpcpd": "*",
    "degordian/php-coding-standards": "^1.0",
    "flow/jsonpath": "*",
    "phploc/phploc": "*",
    "pdepend/pdepend": "^2.5",
    "sensiolabs/security-checker": "*",
    "phpbench/phpbench": "*",
    "phpstan/phpstan": "^0.10.3"
  },
  "authors": [
    {
      "name": "Marko Kruljac",
      "email": "marko.kruljac@degordian.com"
    }
  ],
  "minimum-stability": "stable"
}
